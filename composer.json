{
    "name": "cmizzi/laravel-command-alias",
    "description": "Define aliases for Artisan commands",
    "homepage": "https://github.com/cmizzi/laravel-command-alias",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "illuminate/console": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cmizzi\\CommandAlias\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cmizzi\\CommandAlias\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmizzi\\CommandAlias\\CommandAliasServiceProvider"
            ]
        }
    }
}
